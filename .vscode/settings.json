{
	"scss.lint.unknownAtRules": "ignore",
	"files.associations": {
		"*.css": "postcss"
	},
	"liveSassCompile.settings.formats": [
		{
			"format": "expanded",
			"extensionName": ".css",
			"savePath": "/.watch",
			"savePathReplacementPairs": null
		}
	],
	"tailwindCSS.includeLanguages": {
		"astro": "html"
	},
	"editor.quickSuggestions": {
		"strings": true
	},
	"prettier.jsxSingleQuote": true,
	"prettier.singleQuote": true
}
